<!DOCTYPE html>
<html dir="rtl" lang="ar">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ناجح</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/css/navigation.css" rel="stylesheet" />
    <link href="~/css/questions/questions.css" rel="stylesheet" />
    @RenderSection("Css", required: false)
</head>

<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="top-menu mt-1">
            <div><a href="/">الرئيسية</a></div>
            <div><a href="/quiz">تدريبات</a></div>
            <div><a href="/exams">اختبارات</a></div>
            @*<div><a href="/about" id="contact-clk">نبذه عن الموقع</a></div>*@
            @*<div><a href="/contactus" id="contact-clk">اتصل بنا</a></div>*@
        </div>
        <img class="logo" src="https://cdn3.iconfinder.com/data/icons/sport-free/24/Sport__Sport_Archery_Focus_Goal_Success-512.png" />
        @*<partial name="_LoginPartial" />*@
    </nav>

    <partial name="_CookieConsentPartial" />

    <div class="container body-content">
        <div class="content-area">
            @RenderBody()
        </div>

        <hr />
        <footer>
            <p>ناجح - &copy; 2021</p>
        </footer>
    </div>

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/js/utils.js"></script>
    <script src="~/js/db.js"></script>
    <script src="~/js/navigation.js"></script>
    <script src="~/js/feedback.js"></script>
    <script src="~/js/questions/choice.js"></script>
    <script src="~/js/questions/kasr.js"></script>
    <script src="~/js/questions/gadwal.js"></script>
    <script src="~/js/questions/addition.js"></script>
    <script src="~/js/questions/multiplication.js"></script>
    <script src="~/js/questions/division.js"></script>
    <script src="~/js/questions/subtraction.js"></script>
    <script src="~/js/questions/english/englishChoice.js"></script>
    <script src="~/js/questions/english/ArabicEnglishChoice.js"></script>
    <script src="~/js/questions/english/EnglishArabicChoice.js"></script>
    <script type="text/javascript" src="~/js/quiz.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>