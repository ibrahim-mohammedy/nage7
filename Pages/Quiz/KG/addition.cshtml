@page
@model AdditionQuizModel
@{
    ViewData["Title"] = "تدريبات علي عملية الجمع";
}

@section Css{
    <style>

        .op {
            color: red;
        }

        .mult-header {
            text-align: center;
            font-size: x-large;
        }

        .contact-us {
            float: right;
            font-size: large;
        }
    </style>
}

@section Scripts{
    <script src="~/js/db.js"></script>
    <script src="~/js/feedback.js"></script>
    <link href="~/css/questions/questions.css" rel="stylesheet" />
    <script src="~/js/questions/kasr.js"></script>
    <script src="~/js/questions/gadwal.js"></script>
    <script src="~/js/questions/addition.js"></script>
    <script type="text/javascript" src="~/js/quiz.js"></script>
    <script>

        $(function () {
            var quiz = new Quiz('quizArea');
            quiz.questionsTypes = [new QuestionCategory(QuestionsTypes.Addition, 'عملية الجمع', 0)];
            quiz.show('quizArea');
        });
    </script>
}
<div class="row">
    <h3 class="page-title">تدريبات علي عملية الجمع</h3>
    <div id="quizArea">
    </div>
</div>